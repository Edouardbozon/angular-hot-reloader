{
    "compileOnSave": false,
    "compilerOptions": {
        "outDir": "./lib/",
        "preserveConstEnums": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "removeComments": false,
        "noImplicitAny": false,
        "sourceMap": true,
        "declaration": false,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react"
    },
    "include": [
        "app/**/*"
    ],
    "types": [
       "core-js"
    ]
}